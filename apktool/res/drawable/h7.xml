<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/ow" />
            <stroke android:width="1.0dip" android:color="@color/h2" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/h2" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/h1" />
            <stroke android:width="1.0dip" android:color="@color/h1" />
        </shape>
    </item>
</selector>