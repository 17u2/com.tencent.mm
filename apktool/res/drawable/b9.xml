<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/bb" />
            <stroke android:width="@dimen/d1" android:color="@color/b2" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/bd" />
            <stroke android:width="@dimen/d1" android:color="@color/b4" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/bc" />
            <stroke android:width="@dimen/d1" android:color="@color/b3" />
        </shape>
    </item>
</selector>