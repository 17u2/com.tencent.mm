<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/nl" />
            <stroke android:width="@dimen/d1" android:color="@color/ax" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/nl" />
            <stroke android:width="@dimen/d1" android:color="@color/ay" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/d0" />
            <solid android:color="@color/nl" />
            <stroke android:width="@dimen/d1" android:color="@color/aw" />
        </shape>
    </item>
</selector>